<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.holeinone.ssafit.model.dao.VideoDao">

    <!--유튜브 랜덤 영상 저장-->
    <insert id="insertVideoRoutine" parameterType="com.holeinone.ssafit.model.dto.Videos">
        INSERT INTO videos (
            source_type,
            video_url,
            title,
            duration_seconds,
            channel_name,
            user_id
        ) VALUES (
                     #{sourceType},
                     #{videoUrl},
                     #{title},
                     #{durationSeconds},
                     #{channelName},
                     #{userId}
                 )
    </insert>

</mapper>